{
    "name": "Hi9",
    "description": "Hi9 is a way for people to be connected with what matters",
    "version": "1.0",
    "incognito": "split",
    "chrome_url_overrides": { "newtab": "redirect.html" },
    "manifest_version": 2,
    "permissions": ["tabs","idle","activeTab","https://hi9.uk/","bookmarks","storage"],  
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "page": ["hi9.html"]
    }
}
